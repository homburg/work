{
  "name": "cross-repo-sync-action",
  "version": "1.0.0",
  "description": "Syncs and renames files or folders from your repo to a different repository and branch",
  "main": "src/main.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "start": "node --experimental-strip-types src/main.ts",
    "test": "node --experimental-strip-types --test",
    "test:watch": "yarn test --watch",
    "test:update": "yarn test --test-update-snapshots"
  },
  "type": "module",
  "keywords": [
    "github-action",
    "sync",
    "cross-repo",
    "automation",
    "ci-cd"
  ],
  "author": "github-actions[bot]",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@effect/cluster": "^0.46.2",
    "@effect/experimental": "^0.54.3",
    "@effect/opentelemetry": "^0.56.1",
    "@effect/platform": "^0.90.0",
    "@effect/platform-node": "^0.94.1",
    "@effect/rpc": "^0.68.2",
    "@effect/sql": "^0.44.0",
    "@effect/workflow": "^0.8.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "effect": "^3.17.6"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "memfs": "^4.36.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
